{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
      "types": ["cypress", "node", "cy-verify-downloads", "cypress-delete-downloads-folder"],
      "paths": {
          "@pages/*": ["./cypress/support/pages/*"],
          "@data/*": ["./cypress/fixtures/data/*"],
          "@utils": ["./cypress/support/utils/*"],
      }
  },
  "include": ["**/*.ts","cypress.d.ts"],
  "exclude": ["node_modules"]
  
}
